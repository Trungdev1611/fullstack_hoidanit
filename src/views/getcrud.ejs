<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <style>
        table {
            width: 100%;
            border-collapse: collapse;
        }

        tr,
        table,
        td,
        th {
            border: 1px solid red;
            padding: 5px;
        }
    </style>
</head>

<body>
    <table>
        <tr>
            <th>id</th>
            <th>firstName</th>
            <th>lastName</th>
            <th>Email</th>
            <th>Action</th>
        </tr>
        <% for(var i=0; i < data.length; i++) { %>
            <tr>
                <td>
                    <%= data[i].id %>
                </td>
                <td>
                    <%= data[i].firstName %>
                </td>
                <td>
                    <%= data[i].lastName %>
                </td>
                <td>
                    <%= data[i].email %>
                </td>
                <td>
                    <a href="/edit-user/?id=<%=data[i].id%>">Edit user</a>
                    <a href="/delete-user/?id=<%=data[i].id%>">Delete user</a>
                </td>
            </tr>
            <% } %>
    </table>
</body>

</html>